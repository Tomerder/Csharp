<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Todo.App.Modules.Category">
    <local:EditCategoryVm 
        x:Key="dt.EditCategoryVm"
        DisplayName="Shopping"
        Color="Purple"
        Uid="1"       
        >
        <local:EditCategoryVm.Items>
            <local:ItemVm DisplayName="First thing" IsDone="False" Priority="Immediate"/>
            <local:ItemVm DisplayName="Second item" IsDone="True" Priority="Attention"/>
            <local:ItemVm DisplayName="Third thing" IsDone="False" Priority="Attention"/>
            <local:ItemVm DisplayName="Last thing" IsDone="False" Priority="Urgent"/>
        </local:EditCategoryVm.Items>
    </local:EditCategoryVm>

    <local:ItemVm x:Key="dt.Category.ItemVm"
                  DisplayName="Buy Milk"
                  Priority="Immediate"
                  IsDone="False"
                  Color="Purple"
                  />







</ResourceDictionary>